{
  "name": "aflr",
  "version": "0.0.4",
  "description": "Aflorithmic NodeJS SDK",
  "author": "salih <salih@aflorithmic.ai>",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "aflr",
    "npm",
    "aflorithmic",
    "package"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aflorithmic/aflr_npm.git"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push origin master && git push origin master --tags"
  },
  "bugs": {
    "url": "https://github.com/aflorithmic/aflr_npm/issues"
  },
  "homepage": "https://github.com/aflorithmic/aflr_npm#readme",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "codecov": "^3.1.0",
    "jest": "^23.6.0",
    "prettier": "^1.13.2",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.8.3"
  },
  "files": [
    "lib/**/*"
  ]
}
